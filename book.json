{
  "title": "jie6mm",
  "author": "jie6mm",
  "description": "记录一些个人及学习相关笔录",
  "language": "zh-hans",
  "plugins": [
    "-search",
    "-lunr",
    "-sharing",
    "search-pro",
    "sharing-plus",
    "lightbox",
    "splitter",
    "chapter-fold",
    "code",
    "advanced-emoji",
    "klipse",
    "pageview-count",
    "click-reveal",
    "anchor-navigation-ex"
  ],
  "pluginsConfig": {
    "anchor-navigation-ex": {
      "showLevel": false,
      "showGoTop": false
    },
    "code": {
      "copyButtons": true
    },
    "sharing": {
      "facebook": true,
      "twitter": true,
      "all": [
        "weibo",
        "douban",
        "qq",
        "qzone",
        "linkedin",
        "twitter",
        "facebook"
      ]
    }
  }
}

